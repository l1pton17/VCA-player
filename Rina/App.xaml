<Application x:Class="Rina.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Rina.StyleResources;component/Styles/Colors.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Rina.StyleResources;component/Styles/Accents/Blue.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Rina.StyleResources;component/Styles/Accents/BaseLight.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Rina.StyleResources;component/Styles/Controls.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Rina.StyleResources;component/Icons/Icons.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
